<?xml version="1.0" encoding="utf-8" ?>
<Plugin xmlns="http://schemas.monoflector.org/plugins/2011/definition"
        Identity="plugins.monoflector.org/Languages/CSharp"
        Version="1.0.0.0">
  <Title>CSharp Language</Title>
  <Author>Monoflector Project</Author>
  <Description>Support for decompiling to C#.</Description>
  <InstallDocumentation>InstallDoc</InstallDocumentation>

  <Deployment xmlns:files="http://schemas.monoflector.org/plugins/2011/deployment/files"
              xmlns:mef="http://schemas.monoflector.org/plugins/2011/deployment/mef">

    <files:copy Filename="Monoflector.CSharp.dll" />
    <files:copy Filename="Monoflector.CSharp.pdb" />

    <mef:activate Filename="Monoflector.CSharp.dll" Context="Normal" />

  </Deployment>

</Plugin>